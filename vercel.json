{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "YOUTUBE_API_KEY": "@youtube_api_key"
  },
  "functions": {
    "app.py": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
